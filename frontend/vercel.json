{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://shula-rent-project-production.up.railway.app"
  },
  "alias": ["shula-webapp.vercel.app"],
  "name": "shula-webapp"
} 